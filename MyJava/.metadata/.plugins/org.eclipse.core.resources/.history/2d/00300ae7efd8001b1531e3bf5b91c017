package com.basic.factorial;

public class FactorialCmd {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		long n,fact=1;
		n=Long.parseLong(args[0]);
		System.out.println("n="+n);
		for(int i=1;i<=n;i++){
			fact=fact*i;
		}
		System.out.println("Factorial ="+fact);

	}

}
